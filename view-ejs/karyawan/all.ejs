<h1> Dsts Seluruh Karyawan PT Indo Teknik</h1>
<hr><hr><br>
<table border="1">
    <thead>
        <tr>
            <th>Nama</th>
            <th>Alamat</th>
            <th>Jenis Kelamin</th>
            <th>Departemen</th>
            <th>Jabatan</th>
            <th>Opsi</th>
        </tr>
    </thead>

    <tbody>
        <%for (const i in semua_karyawan){%>
            <tr>
                <td> <%= semua_karyawan[i].nama%></td>
                <td> <%= semua_karyawan[i].alamat%></td>
                <td> <%= semua_karyawan[i].jenis_kelamin%></td>
                <td> <%= semua_karyawan[i].departemen_kode%></td>
                <td> <%= semua_karyawan[i].jabatan_nama%></td>
                <td>
                    <a href="/karyawan/detail">
                        <button>detail</button>
                    </a>
                </td>
            </tr>
        <%}%>

        <%#
        for (const i in semua_karyawan){
            __append(`<tr>`)
            __append(`<td>${semua_karyawan[i].nama}</td>`)
            __append(`<td>${semua_karyawan[i].alamat}</td>`)
            __append(`<td>${semua_karyawan[i].no_telepon}</td>`)
            __append(`<td>${semua_karyawan[i].gol_darah}</td>`)
            __append(`<td>${semua_karyawan[i].jenis_kelamin}</td>`)
            __append(`<td>${semua_karyawan[i].departemen_id}</td>`)
            __append(`<td>${semua_karyawan[i].jabatan_id}</td>`)
        }
        %>
    </tbody>
</table>

